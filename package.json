{
  "name": "lazy-jest",
  "version": "0.0.0-alpha.1",
  "description": "Jest util for lazy person like me to generate mass test snapshots with few configurations.",
  "main": "lazy-jest.js",
  "scripts": {
    "prepublishOnly": "npm run docs & npm run build",
    "build": "WEBPACK_ENV=build ./node_modules/.bin/webpack -p --display-error-details --config ./webpack.config.js",
    "docs": "jsdoc2md --template README.hbs 'src/**/*.js' > README.md --separators -c ./.jsdoc.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "jest",
    "test"
  ],
  "files": [
    "lazy-jest.js",
    "lazy-jest.js.map"
  ],
  "license": "MIT",
  "dependencies": {
    "jest": "^23.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "flow-bin": "^0.78.0",
    "flow-typed": "^2.5.1",
    "jest": "^23.4.2",
    "jsdoc-babel": "^0.4.0",
    "jsdoc-to-markdown": "^4.0.1",
    "lerna": "^3.0.0-beta.21",
    "prettier": "^1.14.0",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8"
  },
  "devEngines": {
    "node": "8.x || 9.x || 10.x"
  },
  "author": {
    "name": "Jennie Ji",
    "email": "jennie.ji@hotmail.com"
  }
}
