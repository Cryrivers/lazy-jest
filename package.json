{
  "name": "lazy-jest",
  "version": "0.1.0-beta",
  "description": "Jest util for lazy person like me to generate mass test snapshots with few configurations.",
  "main": "dist/lazy-jest.js",
  "browser": "dist/lazy-jest.umd.js",
  "modules": "dist/lazy-jest.esm.js",
  "scripts": {
    "prepublishOnly": "npm run docs & npm run build",
    "build": "NODE_ENV=production rollup -c",
    "docs": "jsdoc2md --template README.hbs 'src/**/*.js' > README.md --separators -c ./.jsdoc.json",
    "test": "BABLE_ENV=test NODE_ENV=testing jest --env=node",
    "coverage": "yarn test --coverage --coverageReporters=lcov"
  },
  "keywords": [
    "jest",
    "test",
    "productive",
    "javascript"
  ],
  "files": [
    "lazy-jest.js",
    "lazy-jest.js.map"
  ],
  "license": "MIT",
  "dependencies": {
    "jest": "^23.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "flow-bin": "^0.78.0",
    "flow-typed": "^2.5.1",
    "from": "^0.1.7",
    "import": "^0.0.6",
    "jsdoc-babel": "^0.4.0",
    "jsdoc-to-markdown": "^4.0.1",
    "prettier": "^1.14.0",
    "resolve": "^1.8.1",
    "rollup": "^0.64.1",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.5",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-terser": "^1.0.1"
  },
  "devEngines": {
    "node": "8.x || 9.x || 10.x"
  },
  "author": {
    "name": "Jennie Ji",
    "email": "jennie.ji@hotmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JennieJi/lazy-jest"
  },
  "bugs": {
    "url": "https://github.com/JennieJi/lazy-jest/issues"
  }
}
